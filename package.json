{
  "name": "blockchain",
  "version": "1.0.0",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "private-network": "ganache-cli --time 1970-01-01T00:00:00Z",
    "test": "truffle test",
    "test-local": "ganache-cli --time 1970-01-01T00:00:00Z -q & truffle test",
    "test-mcdWrapper": "truffle test --network kovan test/mcdWrapperTest/*.js",
    "test-integration": "truffle test --network kovan test/integration/*.js",
    "compile": "truffle compile",
    "validate-tests": "eslint *.js test/*.js test/helpers/*.js test/mcdWrapperTest --fix",
    "validate-sol": "solium --dir contracts --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.s-pro.io/akiva/blockchain.git"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bignumber.js": "^9.0.0",
    "dotenv": "^8.1.0",
    "eslint-config-google": "^0.14.0",
    "ganache-cli": "^6.7.0",
    "solidity-coverage": "^0.6.7",
    "solium": "^1.2.5",
    "truffle": "^5.1.8",
    "truffle-flattener": "^1.4.2",
    "truffle-hdwallet-provider": "^1.0.17",
    "web3": "^1.2.1",
    "zos": "^2.4.2"
  },
  "devDependencies": {
    "eslint": "^6.5.1",
    "truffle-plugin-solhint": "0.0.6"
  }
}
